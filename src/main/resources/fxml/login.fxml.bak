<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane prefHeight="771.0" prefWidth="1048.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.wayruha.controller.MainController">
  <center>
    <VBox prefHeight="663.0" prefWidth="878.0" spacing="10.0">
      <children>
        <TableView fx:id="table" minWidth="100.0" onMouseClicked="#handleTableClick" prefHeight="432.0" prefWidth="400.0" />
        <HBox prefHeight="70.0" prefWidth="858.0">
          <children>
            <TextArea editable="false" prefHeight="70.0" prefWidth="806.0" wrapText="true" />
            <VBox prefHeight="200.0" prefWidth="100.0">
              <children>
                <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@images/up.png" preserveRatio="false" smooth="false" />
                  </image>
                </ImageView>
                <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@images/down.png" preserveRatio="false" smooth="false" />
                  </image>
                </ImageView>
              </children>
            </VBox>
          </children>
        </HBox>
        <HBox prefHeight="56.0" prefWidth="858.0">
          <children>
            <Button fx:id="prevButt" mnemonicParsing="false" onAction="#handlePrevButt" text="Prev Butt" />
            <Button fx:id="nextButt" mnemonicParsing="false" onAction="#handleNextButt" text="Next Butt">
              <HBox.margin>
                <Insets left="30.0" />
              </HBox.margin>
            </Button>
            <Button fx:id="addButt" mnemonicParsing="false" onAction="#handleAddButt" text="Add Button">
              <HBox.margin>
                <Insets left="500.0" right="10.0" />
              </HBox.margin>
            </Button>
          </children>
          <VBox.margin>
            <Insets />
          </VBox.margin>
        </HBox>
      </children>
      <padding>
        <Insets right="20.0" />
      </padding>
    </VBox>
  </center>
  <left>
    <VBox fx:id="patternBox" prefHeight="671.0" prefWidth="170.0" />
  </left>
  <top>
    <FlowPane minHeight="70.0" prefHeight="70.0" prefWidth="1048.0" style="  -fx-border-color: -fx-text-box-border;   -fx-border-width: 2;">
      <children>
        <MenuBar prefWidth="1048.0">
          <menus>
            <Menu mnemonicParsing="false" text="File">
              <items>
                <MenuItem mnemonicParsing="false" text="Close" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
              <items>
                <MenuItem mnemonicParsing="false" text="Delete" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
              <items>
                <MenuItem mnemonicParsing="false" text="About" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
        <HBox prefHeight="55.0" prefWidth="1048.0">
          <children>
            <ImageView fitHeight="46.0" fitWidth="61.33333333333333" onMouseClicked="Test.handler" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@images/addPattern.png" preserveRatio="false" smooth="false" />
              </image>
            </ImageView>
            <Label text="Введіть шуканий товар">
              <HBox.margin>
                <Insets left="180.0" top="15.0" />
              </HBox.margin>
            </Label>
            <TextField prefHeight="24.0" prefWidth="422.0">
              <HBox.margin>
                <Insets left="20.0" top="15.0" />
              </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" prefHeight="24.0" prefWidth="76.0" text="Button" textOverrun="CLIP">
              <HBox.margin>
                <Insets left="25.0" top="10.0" />
              </HBox.margin>
            </Button>
          </children>
        </HBox>
      </children>
    </FlowPane>
  </top>
</BorderPane>
